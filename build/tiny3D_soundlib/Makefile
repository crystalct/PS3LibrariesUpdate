
.PHONY: all pkg clean

all:
	
	$(MAKE) install -C ps3soundlib/libs
	$(MAKE) install -C lib
	$(MAKE) install -C libfont

pkg:
	$(MAKE) install -C ps3soundlib/libs
	$(MAKE) install -C lib
	$(MAKE) install -C libfont

clean:
	$(MAKE) clean -C ps3soundlib/libs
	$(MAKE) clean -C lib
	$(MAKE) clean -C libfont


